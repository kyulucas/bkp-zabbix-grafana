            "rawQuery": true,
            "rawSql": "SELECT \r\n    Data,\r\n    SUM(Abertas) AS Abertas,\r\n    SUM(Baixadas) AS Baixadas\r\nFROM (\r\n    SELECT \r\n        DATE_FORMAT(DATA_CADASTRO, '%d/%m/%Y') AS Data, \r\n        COUNT(*) AS Abertas, \r\n        0 AS Baixadas\r\n    FROM ALTAREDE.OS \r\n    WHERE COD_TIPOSERVICO = 2 \r\n      AND EXCLUIDO = 'N' \r\n      AND DATA_CADASTRO >= '${__from:date:YYYY-MM-DD 00:00:00}' \r\n      AND DATA_CADASTRO <= '${__to:date:YYYY-MM-DD 23:59:59}'\r\n    GROUP BY DATE_FORMAT(DATA_CADASTRO, '%d/%m/%Y')\r\n    \r\n    UNION ALL\r\n    \r\n    SELECT \r\n        DATE_FORMAT(DATA_CONCLUSAO, '%d/%m/%Y') AS Data, \r\n        0 AS Abertas, \r\n        COUNT(*) AS Baixadas\r\n    FROM ALTAREDE.OS  \r\n    WHERE COD_TIPOSERVICO = 2 \r\n      AND EXCLUIDO = 'N' \r\n      AND DATA_CONCLUSAO >= '${__from:date:YYYY-MM-DD 00:00:00}' \r\n      AND DATA_CONCLUSAO <= '${__to:date:YYYY-MM-DD 23:59:59}'\r\n    GROUP BY DATE_FORMAT(DATA_CONCLUSAO, '%d/%m/%Y')\r\n) AS t\r\nGROUP BY Data\r\nORDER BY STR_TO_DATE(Data, '%d/%m/%Y');\r\n",
