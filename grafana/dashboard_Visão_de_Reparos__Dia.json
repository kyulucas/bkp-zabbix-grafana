            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT \r\n    a.ID_OS as OS, \r\n        DATE_FORMAT(a.DATA_CADASTRO,'%d/%m/%Y') as ABERTURA, \r\n            a.LOGIN, \r\n                c.NOME as tecnico, \r\n                    b.BAIRRO, \r\n                        b.CIDADE, \r\n                            a.PRIORIDADE,\r\n                                DATEDIFF(CURDATE(), a.DATA_CADASTRO) - 2 as `Dias em atraso`\r\n                                FROM OS as a \r\n                                LEFT JOIN ENDERECO_LOGIN as b ON b.LOGIN = a.LOGIN\r\n                                LEFT JOIN FUNCIONARIO as c ON c.COD_FUNCIONARIO = a.COD_TECNICO\r\n                                WHERE a.COD_TIPOSERVICO = 2 \r\n                                  AND a.STATUS = 'A' \r\n                                    AND a.EXcluido = 'N'\r\n                                    GROUP BY OS\r\n                                    ORDER BY PRIORIDADE desc, `Dias em atraso` desc\r\n",
            "refId": "A",
            "sql": {
              "columns": [
