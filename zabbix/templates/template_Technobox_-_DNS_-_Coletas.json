        "discovery_rules": [
          {
            "uuid": "8a4710a44bf94380a05b1049ede5d67c",
            "name": "Descoberta de Interfaces de Rede",
            "type": "SNMP_AGENT",
            "snmp_oid": "discovery[{#IFDESCR},1.3.6.1.2.1.2.2.1.2,{#IFTYPE},1.3.6.1.2.1.2.2.1.3]",
            "key": "net.if.discovery",
            "delay": "1h",
            "filter": {
              "conditions": [
                {
                  "macro": "{#IFTYPE}",
                  "value": "^6$",
                  "formulaid": "A"
                }
              ]
            },
            "lifetime": "30d",
            "enabled_lifetime_type": "DISABLE_NEVER",
            "item_prototypes": [
              {
                "uuid": "9334ba8846ff467387ea4be263f90138",
                "name": "Interface {#IFDESCR} - Status Administrativo",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.2.1.2.2.1.7.{#SNMPINDEX}",
                "key": "net.if.admin.status[{#IFDESCR}]",
                "history": "7d",
                "trends": "0",
                "description": "Status administrativo da interface {#IFDESCR}. Valores: 1=up (habilitada), 2=down (desabilitada), 3=testing (em teste)."
              },
              {
                "uuid": "bdb2352954d64a4198464a3aaa51ed92",
                "name": "Interface {#IFDESCR} - Descartados de Entrada",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.2.1.2.2.1.13.{#SNMPINDEX}",
                "key": "net.if.in.discards[{#IFDESCR}]",
                "history": "7d",
                "trends": "90d",
                "description": "Pacotes descartados na entrada da interface {#IFDESCR}. Contador de pacotes recebidos mas descartados (ifInDiscards)."
              },
              {
                "uuid": "30e878f74c7647618c66c35198560ce1",
                "name": "Interface {#IFDESCR} - Erros de Entrada",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.2.1.2.2.1.14.{#SNMPINDEX}",
                "key": "net.if.in.errors[{#IFDESCR}]",
                "history": "7d",
                "trends": "90d",
                "description": "Erros de entrada na interface {#IFDESCR}. Contador de pacotes recebidos com erros (ifInErrors)."
              },
              {
                "uuid": "63ba2773795846b79e5cd400ecd5216c",
                "name": "Interface {#IFDESCR} - Tráfego de Entrada",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.2.1.2.2.1.10.{#SNMPINDEX}",
                "key": "net.if.in[{#IFDESCR}]",
                "history": "7d",
                "trends": "90d",
                "units": "bps",
                "description": "Tráfego de entrada na interface {#IFDESCR} em bits por segundo. Contador de octetos recebidos na interface (ifInOctets) convertido para taxa por segundo e multiplicado por 8 para obter bits.",
                "preprocessing": [
                  {
                    "type": "CHANGE_PER_SECOND",
                    "parameters": [
                      ""
                    ]
                  },
                  {
                    "type": "MULTIPLIER",
                    "parameters": [
                      "8"
                    ]
                  }
                ]
              },
              {
                "uuid": "97bf68efddc8409ba2e3577f48e9b786",
                "name": "Interface {#IFDESCR} - Descartados de Saída",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.2.1.2.2.1.19.{#SNMPINDEX}",
                "key": "net.if.out.discards[{#IFDESCR}]",
                "history": "7d",
                "trends": "90d",
                "description": "Pacotes descartados na saída da interface {#IFDESCR}. Contador de pacotes para envio mas descartados (ifOutDiscards)."
              },
              {
                "uuid": "03e161a9ec444bdd86c1c7217d956070",
                "name": "Interface {#IFDESCR} - Erros de Saída",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.2.1.2.2.1.20.{#SNMPINDEX}",
                "key": "net.if.out.errors[{#IFDESCR}]",
                "history": "7d",
                "trends": "90d",
                "description": "Erros de saída na interface {#IFDESCR}. Contador de pacotes enviados com erros (ifOutErrors)."
              },
              {
                "uuid": "44e3e2548ae348269d61f23507e23b0a",
                "name": "Interface {#IFDESCR} - Tráfego de Saída",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.2.1.2.2.1.16.{#SNMPINDEX}",
                "key": "net.if.out[{#IFDESCR}]",
                "history": "7d",
                "trends": "90d",
                "units": "bps",
                "description": "Tráfego de saída na interface {#IFDESCR} em bits por segundo. Contador de octetos enviados na interface (ifOutOctets) convertido para taxa por segundo e multiplicado por 8 para obter bits.",
                "preprocessing": [
                  {
                    "type": "CHANGE_PER_SECOND",
                    "parameters": [
                      ""
                    ]
                  },
                  {
                    "type": "MULTIPLIER",
                    "parameters": [
                      "8"
                    ]
                  }
                ]
              },
              {
                "uuid": "2ad94462d4044704b2047dca4253103c",
                "name": "Interface {#IFDESCR} - Velocidade",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.2.1.2.2.1.5.{#SNMPINDEX}",
                "key": "net.if.speed[{#IFDESCR}]",
                "delay": "1h",
                "history": "30d",
                "trends": "0",
                "units": "bps",
                "description": "Velocidade máxima da interface {#IFDESCR} em bits por segundo (ifSpeed)."
              },
