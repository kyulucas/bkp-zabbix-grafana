            "key": "contpon",
            "delay": "2m",
            "history": "1d",
            "value_type": "TEXT",
            "trends": "0",
            "preprocessing": [
              {
                "type": "STR_REPLACE",
                "parameters": [
                  "{#SNMPINDEX}",
                  "PON"
                ]
              },
              {
                "type": "STR_REPLACE",
                "parameters": [
                  "{#STATUS}",
                  "STATUS"
                ]
              },
              {
                "type": "JAVASCRIPT",
                "parameters": [
                  "valueObj = JSON.parse(value);\nvalueObj.forEach(function (el) {\n    el[\"PON\"] = el[\"PON\"].replace(/\\.(\\d+)/g, \"\");\n});\nreturn JSON.stringify(valueObj);"
                ]
              }
            ],
            "tags": [
