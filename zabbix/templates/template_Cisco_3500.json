                "uuid": "9f1b7b73aa4543a1b714b2e1d54c148a",
                "name": "CPU #{#SNMPINDEX} Usage 1min",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.4.1.9.9.109.1.1.1.1.7.{#SNMPINDEX}",
                "key": "cpmCPUTotal1minRev[{#SNMPINDEX}]",
                "delay": "60",
                "units": "%",
                "description": "CISCO-PROCESS-MIB::cpmCPUTotal1minRev\n\nThe overall CPU busy percentage in the last 1 minute period. OID: .1.3.6.1.4.1.9.9.109.1.1.1.1.7.1",
                "tags": [
                  {
                    "tag": "Application",
                    "value": "CPU"
                  }
                ]
              },
              {
                "uuid": "be44a950d6c84eebb52cf37aa1e965ca",
                "name": "CPU #{#SNMPINDEX} Usage 5min",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.4.1.9.9.109.1.1.1.1.8.{#SNMPINDEX}",
                "key": "cpmCPUTotal5minRev[{#SNMPINDEX}]",
                "delay": "300",
                "units": "%",
                "description": "CISCO-PROCESS-MIB::cpmCPUTotal5minRev\n\nThe overall CPU busy percentage in the last 5 minute period.",
                "tags": [
                  {
                    "tag": "Application",
                    "value": "CPU"
                  }
                ],
                "trigger_prototypes": [
                  {
                    "uuid": "aac129b21a334eeaad4b4c706841736b",
                    "expression": "last(/Cisco_3500/cpmCPUTotal5minRev[{#SNMPINDEX}])>95",
                    "name": "High CPU #{#SNMPINDEX} Usage",
                    "priority": "HIGH"
                  }
                ]
              },
              {
                "uuid": "ddea3749311043d89f3c7ca17c7f4a67",
                "name": "CPU #{#SNMPINDEX} Usage 5sec",
                "type": "SNMP_AGENT",
                "snmp_oid": ".1.3.6.1.4.1.9.9.109.1.1.1.1.6.{#SNMPINDEX}",
                "key": "cpmCPUTotal5secRev[{#SNMPINDEX}]",
                "delay": "30",
                "status": "DISABLED",
                "units": "%",
                "description": "CISCO-PROCESS-MIB::cpmCPUTotal5secRev\n\nThe overall CPU busy percentage in the last 5 second period.",
                "tags": [
                  {
                    "tag": "Application",
                    "value": "CPU"
                  }
                ]
              }
            ],
            "graph_prototypes": [
              {
                "uuid": "cdce2790c12740c880f2b73b49b84a36",
                "name": "CPU #{#SNMPINDEX} Usage",
                "graph_items": [
                  {
                    "color": "00C800",
                    "item": {
                      "host": "Cisco_3500",
                      "key": "cpmCPUTotal1minRev[{#SNMPINDEX}]"
                    }
                  },
                  {
                    "sortorder": "1",
                    "color": "C80000",
                    "item": {
                      "host": "Cisco_3500",
                      "key": "cpmCPUTotal5minRev[{#SNMPINDEX}]"
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
